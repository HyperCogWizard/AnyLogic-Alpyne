		<ReinforcementLearningExperiment ActiveObjectClassId="1347625978602">
			<Id>1620144819661</Id>
			<Name><![CDATA[RLExperiment]]></Name>
			<MaximumMemory>512</MaximumMemory>
			<RandomNumberGenerationType>randomSeed</RandomNumberGenerationType>
			<CustomGeneratorCode>new Random()</CustomGeneratorCode>
			<SeedValue>1</SeedValue>
			<ModelTimeProperties>
				<StopOption><![CDATA[Never]]></StopOption>
				<InitialDate><![CDATA[1577836800000]]></InitialDate>	
				<InitialTime><![CDATA[0.0]]></InitialTime>	
				<FinalDate><![CDATA[1640995200000]]></FinalDate>	
				<FinalTime><![CDATA[731.0]]></FinalTime>	
			</ModelTimeProperties>
			<ObservationField>
				<Name><![CDATA[stock]]></Name>
				<Type><![CDATA[double]]></Type>
			</ObservationField>
			<ObservationField>
				<Name><![CDATA[last_stock]]></Name>
				<Type><![CDATA[double]]></Type>
			</ObservationField>
			<ObservationField>
				<Name><![CDATA[order_rate]]></Name>
				<Type><![CDATA[double]]></Type>
			</ObservationField>
			<ObservationField>
				<Name><![CDATA[demand]]></Name>
				<Type><![CDATA[double]]></Type>
			</ObservationField>
			<ObservationCode><![CDATA[stock = root.Stock;
last_stock = root.stockAtLastAction;
order_rate = root.OrderRate;

// technically hidden information; provided for user monitoring during training,
//	but not intended to be passed to the policy
demand = root.Demand;]]></ObservationCode>
			<StopCondition><![CDATA[root.stockAmountStats.min() < root.stopConditionLimits[0] || root.stockAmountStats.max() > root.stopConditionLimits[1]]]></StopCondition>
			<ActionField>
				<Name><![CDATA[order_rate]]></Name>
				<Type><![CDATA[double]]></Type>
			</ActionField>
			<ActionCode><![CDATA[root.updateAssignedRate(limit(0, order_rate, 50));]]></ActionCode>
			<ConfigurationField>
				<Name><![CDATA[acquisition_lag_days]]></Name>
				<Type><![CDATA[int]]></Type>
			</ConfigurationField>
			<ConfigurationField>
				<Name><![CDATA[action_recurrence_days]]></Name>
				<Type><![CDATA[double]]></Type>
			</ConfigurationField>
			<ConfigurationField>
				<Name><![CDATA[stop_condition_limits]]></Name>
				<Type><![CDATA[double[]]]></Type>
			</ConfigurationField>
			<ConfigurationField>
				<Name><![CDATA[demand_volatility]]></Name>
				<Type><![CDATA[double]]></Type>
			</ConfigurationField>
			<ConfigurationCode><![CDATA[// validate input
if (acquisition_lag_days < 1) {
	root.warning("Provided lag input (%s) cannot be < 1; clamping at minimum (1).", acquisition_lag_days);
	acquisition_lag_days = 1;
}
root.acquisitionLag = acquisition_lag_days; 

if (action_recurrence_days < 1) {
	root.warning("Provided action recurrence (%s) cannot be < 1; clamping at minimum (1).", action_recurrence_days);
	action_recurrence_days = 1;
}
root.actionRecurrence = action_recurrence_days;

if (stop_condition_limits == null || stop_condition_limits.length != 2) {
	root.warning("Provided stop condition limits (%s) is not provided or invalid (expecting a two value array); setting to its default", Arrays.toString(stop_condition_limits));
	stop_condition_limits = new double[]{500d, 9500d};
}
root.stopConditionLimits = stop_condition_limits;

root.volatility = demand_volatility;

// make sure `takeAction` will execute
root.mode = 2; ]]></ConfigurationCode>
		</ReinforcementLearningExperiment>
