					<PublicFlag>false</PublicFlag>
					<PresentationFlag>true</PresentationFlag>
					<ShowLabel>true</ShowLabel>
					<Body><![CDATA[double newRate = OrderRate;
double loLimit = uniform(2000, 4000),
		hiLimit = uniform(6000, 8000);
if (Stock < loLimit)
	newRate *= uniform(1.2, 1.4);
else if (Stock < 5000)
	newRate *= uniform(1.1, 1.25);
else if (Stock >= 5000 && Stock < hiLimit)
	newRate *= uniform(0.75, 0.999);
else if (Stock >= hiLimit)
	newRate *= uniform(0.45, 0.7);
newRate = limit(0, newRate, 50);
return newRate;]]></Body>
